      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     Berater.Nachname, Berater.Vorname, Zeiten.Startzeit, Zeiten.Endzeit, Zeiten.ArbBeschreibung, Zeiten.IDBerater, Projekte.Projektname
FROM         Berater INNER JOIN
                      Zeiten ON Berater.IDBerater = Zeiten.IDBerater INNER JOIN
                      Projekte ON Zeiten.IDProjekt = Projekte.IDProjekte
WHERE     (Zeiten.IDBerater = @IDBerater)</CommandText>
        <DataSourceName>HeckTickConnectionString</DataSourceName>
      </Query>
